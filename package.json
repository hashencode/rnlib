{
  "name": "@hashencode/rnlib",
  "version": "0.1.19",
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start --experimental-debugger",
    "pod-install": "cd ios && RCT_NEW_ARCH_ENABLED=1 bundle exec pod install && cd ..",
    "gradlew-clean": "cd android && ./gradlew clean",
    "test": "jest",
    "prepare": "bob build"
  },
  "dependencies": {
    "@gorhom/portal": "^1.0.14",
    "@react-native-community/hooks": "^3.0.0",
    "@react-navigation/bottom-tabs": "^6.6.1",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/native-stack": "^6.11.0",
    "@react-navigation/stack": "^6.4.1",
    "@reduxjs/toolkit": "^2.2.7",
    "ahooks": "^3.8.1",
    "lodash": "^4.17.21",
    "lucide-react-native": "^0.436.0",
    "react": "18.3.1",
    "react-native": "0.75.4",
    "react-native-actions-sheet": "^0.9.7",
    "react-native-animated-pagination-dot": "^0.4.0",
    "react-native-gesture-handler": "^2.19.0",
    "react-native-haptic-feedback": "^2.3.3",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-orientation-locker-cn": "^1.7.2",
    "react-native-reanimated": "^3.16.1",
    "react-native-reanimated-carousel": "^3.5.1",
    "react-native-safe-area-context": "4.14.0",
    "react-native-size-matters": "^0.4.2",
    "react-native-svg": "^15.7.1",
    "react-native-turbo-image": "^1.21.0",
    "react-native-video": "^6.6.4",
    "react-redux": "^9.1.2",
    "rn-placeholder": "^3.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "0.75.4",
    "@react-native/eslint-config": "^0.75.4",
    "@react-native/metro-config": "0.75.4",
    "@react-native/typescript-config": "0.75.4",
    "@types/lodash": "^4.17.7",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^8.19.0",
    "eslint-plugin-import": "^2.29.1",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-native-builder-bob": "^0.30.2",
    "react-test-renderer": "18.3.1",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  },
  "source": "./src/index.ts",
  "main": "./lib/module/index.js",
  "module": "./lib/module/index.js",
  "types": "./lib/typescript/commonjs/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./lib/typescript/module/index.d.ts",
        "default": "./lib/module/index.js"
      },
      "require": {
        "types": "./lib/typescript/commonjs/index.d.ts"
      }
    }
  },
  "files": [
    "src",
    "lib",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "src/lib"
  ],
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      [
        "module",
        {
          "esm": true
        }
      ],
      [
        "typescript",
        {
          "esm": true
        }
      ]
    ]
  },
  "eslintIgnore": [
    "node_modules/",
    "lib/"
  ]
}
