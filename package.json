{
  "name": "@hashencode/rnlib",
  "version": "0.3.6",
  "scripts": {
    "android": "react-native run-android --terminal=/Applications/Utilities/Terminal.app --port 8083",
    "ios": "react-native run-ios --terminal=/Applications/Utilities/Terminal.app --port 8083",
    "pod-install": "cd ios && yarn ios",
    "gradlew-clean": "cd android && ./gradlew clean",
    "test": "jest",
    "prepare": "bob build"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@d11/react-native-fast-image": "^8.12.0",
    "@gorhom/portal": "^1.0.14",
    "@react-native-community/cli": "20.0.0",
    "@react-native-community/cli-platform-android": "20.0.0",
    "@react-native-community/cli-platform-ios": "20.0.0",
    "@react-native-community/hooks": "^3.0.0",
    "@react-native/babel-preset": "0.82.0",
    "@react-native/eslint-config": "0.82.0",
    "@react-native/metro-config": "0.82.0",
    "@react-native/typescript-config": "0.82.0",
    "@react-navigation/bottom-tabs": "^7.5.0",
    "@react-navigation/native": "^7.1.18",
    "@react-navigation/native-stack": "^7.5.0",
    "@react-navigation/stack": "^7.5.0",
    "@types/jest": "^29.5.13",
    "@types/lodash": "^4.17.16",
    "@types/node": "^22.15.2",
    "@types/react": "^19.1.1",
    "@types/react-test-renderer": "^19.1.0",
    "ahooks": "^3.8.1",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^8.19.0",
    "eslint-plugin-import": "^2.32.0",
    "jest": "^29.6.3",
    "lodash": "^4.17.21",
    "lucide-react-native": "^0.503.0",
    "prettier": "2.8.8",
    "prettier-plugin-organize-imports": "^4.2.0",
    "react": "19.1.1",
    "react-native": "0.82.0",
    "react-native-builder-bob": "^0.40.6",
    "react-native-edge-to-edge": "^1.7.0",
    "react-native-gesture-handler": "^2.21.2",
    "react-native-haptic-feedback": "^2.3.3",
    "react-native-orientation-locker-cn": "^1.7.2",
    "react-native-reanimated": "^4.1.3",
    "react-native-safe-area-context": "5.6.1",
    "react-native-size-matters": "^0.4.2",
    "react-native-svg": "^15.11.2",
    "react-test-renderer": "19.1.1",
    "rn-placeholder": "^3.0.3",
    "typescript": "^5.8.3",
    "react-native-worklets": "^0.6.1"
  },
  "engines": {
    "node": ">=20"
  },
  "source": "./src/index.ts",
  "main": "./lib/module/index.js",
  "types": "./lib/typescript/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/typescript/index.d.ts",
      "default": "./lib/module/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "lib",
    "src"
  ],
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      [
        "module",
        {
          "esm": true
        }
      ],
      "typescript"
    ]
  }
